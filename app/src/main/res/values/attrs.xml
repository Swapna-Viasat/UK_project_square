<?xml version="1.0" encoding="utf-8"?>
<resources>
    <declare-styleable name="JosefinSans">
        <attr name="josefin_style"/>
    </declare-styleable>

    <attr name="josefin_style" format="integer">
        <enum name="bold" value="3" />
        <enum name="italic" value="4" />
        <enum name="light" value="5" />
        <enum name="semiBold" value="6" />
    </attr>

    <declare-styleable name="RatingView">
        <attr name="editable"/>
    </declare-styleable>

    <attr name="editable" format="integer">
        <enum name="no" value="0" />
        <enum name="yes" value="1" />
    </attr>

    <declare-styleable name="GraftrsCounterView">
        <attr name="shape"/>
        <attr name="count" format="string"/>
    </declare-styleable>

    <attr name="shape" format="integer">
        <enum name="square" value="0"/>
        <enum name="circle" value="1"/>
    </attr>
</resources>