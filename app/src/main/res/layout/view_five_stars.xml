<?xml version="1.0" encoding="utf-8"?>
<merge xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:orientation="vertical"
    android:layout_width="match_parent"
    android:layout_height="match_parent">

    <LinearLayout
        android:orientation="horizontal"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:gravity="center_horizontal"
        android:layout_marginTop="@dimen/small_padding">

        <ImageView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:id="@+id/worker_my_account_star_1"
            android:src="@drawable/ic_star_black_24dp"
            android:tint="@color/whiteSquareColor"
            android:tag="1"/>

        <ImageView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:id="@+id/worker_my_account_star_2"
            android:src="@drawable/ic_star_black_24dp"
            android:tint="@color/whiteSquareColor"
            android:tag="2"/>

        <ImageView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:id="@+id/worker_my_account_star_3"
            android:src="@drawable/ic_star_black_24dp"
            android:tint="@color/whiteSquareColor"
            android:tag="3"/>

        <ImageView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:id="@+id/worker_my_account_star_4"
            android:src="@drawable/ic_star_border_black_24dp"
            android:tint="@color/whiteSquareColor"
            android:tag="4"/>

        <ImageView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:id="@+id/worker_my_account_star_5"
            android:src="@drawable/ic_star_border_black_24dp"
            android:tint="@color/whiteSquareColor"
            android:tag="5"/>
    </LinearLayout>

</merge>